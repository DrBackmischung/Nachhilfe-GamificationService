type Query {
    stats: [Stats]
    stat(id: String!): Stats
    statByUser(userId: String!): Stats
}

type Stats {
    id: String
    userId: String!
    registerDate: String!
    learningPoints: Int!
    teachingPoints: Int!
    profilePoints: Int!
}

type Mutation {
    add(userId: String!, registerDate: String!, learningPoints: Int!, teachingPoints: Int!, profilePoints: Int!): Stats!
}